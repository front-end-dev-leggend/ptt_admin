/*-----PAGE-FORM-----////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////*/

[class^="page-"] .form-box{
	padding:0.5em;
    font-family: 'DBHelvethaicaX-Reg';
}
[class^="page-"] .form-box .lang-th:before{
